with [get-window Preferences] {
    get-group Robot | get-button "Pass Robot arguments using arguments file" | uncheck
    get-button Apply | click
    get-button $Apply_and_OK_in_preference| click
}
get-eclipse-window | key-type F11  
wait-for-console

