	//change value to non-existing param 
	get-view Variables | get-tree |	get-item "@{c}" | expand
	
	try -times 3 -delay 500 -command{
		get-view Variables | get-tree | get-item "@{c}/\\[2\\]" | get-property "values['Value']" | equals z | verify-true
