	    get-item "@{c}/\\[1\\]" | get-property "values['Name']" | equals "[1]" | verify-true
	    get-item "@{c}/\\[1\\]" | get-property "values['Value']" | equals middle | verify-true
	    get-item "@{c}/\\[2\\]" | get-property "values['Name']" | equals "[2]" | verify-true
